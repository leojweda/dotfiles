# Must be loaded first.
source "$ZDOTDIR/zinit.zsh"

source "$ZDOTDIR/env.zsh"

source "$ZDOTDIR/homebrew.zsh"

# Must be loaded after homebrew.zsh, but before plugins.zsh.
source "$ZDOTDIR/fzf.zsh"

# Must be loaded after fzf.zsh, but before prompt.zsh.
source "$ZDOTDIR/plugins.zsh"

# Must be loaded after plugins.zsh.
source "$ZDOTDIR/prompt.zsh"

# Must be loaded after plugins.zsh.
source "$ZDOTDIR/appearance.zsh"

source "$ZDOTDIR/aliases.zsh"

source "$ZDOTDIR/functions.zsh"

source "$ZDOTDIR/argcomplete.zsh"

source "$ZDOTDIR/bat.zsh"

source "$ZDOTDIR/gh.zsh"

source "$ZDOTDIR/less.zsh"

source "$ZDOTDIR/mise.zsh"

source "$ZDOTDIR/node.zsh"

source "$ZDOTDIR/python.zsh"

source "$ZDOTDIR/thefuck.zsh"

source "$ZDOTDIR/tlrc.zsh"

source "$ZDOTDIR/tmux.zsh"

source "$ZDOTDIR/wget.zsh"

source "$ZDOTDIR/zoxide.zsh"
