{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:yellow",
          "foreground": "p:base03",
          "style": "diamond",
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ if or .SSHSession .Root }}{{ .UserName }}{{end}}{{ if .SSHSession }}@{{ .HostName }}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "session"
        },
        {
          "background": "p:blue",
          "foreground": "p:base3",
          "properties": {
            "style": "agnoster_full"
          },
          "style": "diamond",
          "template": " {{ path .Path .Location }} ",
          "type": "path"
        },
        {
          "background": "p:green",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:violet{{ end }}",
            "{{ if gt .Behind 0 }}p:magenta{{ end }}"
          ],
          "foreground": "p:base03",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:base03{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:base3{{ end }}",
            "{{ if gt .Ahead 0 }}p:base3{{ end }}"
          ],
          "powerline_symbol": "\ue0b4",
          "properties": {
            "branch_max_length": 25,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "accordion",
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "background": "transparent",
          "foreground": "p:green",
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": false,
            "home_enabled": false
          },
          "style": "plain",
          "template": "\ue718 ",
          "type": "node"
        },
        {
          "background": "transparent",
          "foreground": "p:blue",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": "\ue626 ",
          "type": "go"
        },
        {
          "background": "transparent",
          "foreground": "p:yellow",
          "properties": {
            "display_mode": "files",
            "fetch_version": false,
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": "\ue235 ",
          "type": "python"
        },
        {
          "background": "p:red",
          "foreground": "p:base3",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b6",
          "style": "powerline",
          "template": "\uf00d {{ .Code }} ",
          "type": "status"
        },
        {
          "background": "p:bgHighlight",
          "foreground": "default",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b6",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "type": "executiontime"
        },
        {
          "background": "p:bg",
          "foreground": "default",
          "invert_powerline": true,
          "style": "diamond",
          "template": "{{ .CurrentDate | date \"15:04:05\" }}",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "enable_cursor_positioning": true,
  "final_space": true,
  "palette": {
    "base00": "#657B83",
    "base01": "#586E75",
    "base02": "#073642",
    "base03": "#002B36",
    "base0": "#839496",
    "base1": "#93A1A1",
    "base2": "#EEE8D5",
    "base3": "#FDF6E3",
    "blue": "#268BD2",
    "cyan": "#2AA198",
    "green": "#859900",
    "magenta": "#D33682",
    "orange": "#CB4B16",
    "red": "#DC322F",
    "violet": "#6C71C4",
    "yellow": "#B58900"
  },
  "palettes": {
    "list": {
      "dark": {
        "bg": "p:base03",
        "bgHighlight": "p:base02"
      },
      "light": {
        "bg": "p:base3",
        "bgHighlight": "p:base2"
      }
    },
    "template": "{{ if eq .Env.INTERFACE_STYLE \"dark\" }}dark{{ else }}light{{ end }}"
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:base03",
    "template": "<p:base3,p:blue> > </><p:blue,transparent>\ue0b4</> "
  },
  "tooltips": [
    {
      "background": "p:orange",
      "foreground": "p:base3",
      "leading_diamond": "\ue0b0",
      "properties": {
        "display_default": true
      },
      "style": "diamond",
      "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
      "tips": ["aws"],
      "trailing_diamond": "\ue0b4",
      "type": "aws"
    },
    {
      "background": "p:blue",
      "foreground": "p:base3",
      "leading_diamond": "\ue0b0",
      "properties": {
        "display_default": true
      },
      "style": "diamond",
      "template": " \uebd8 {{ .Name }} ",
      "tips": ["az"],
      "trailing_diamond": "\ue0b4",
      "type": "az"
    }
  ],
  "upgrade": {
    "auto": false,
    "interval": "168h",
    "notice": false,
    "source": "cdn"
  },
  "version": 3
}
