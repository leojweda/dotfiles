{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"background": "p:yellow",
					"foreground": "p:base3",
					"style": "diamond",
					"template": " {{ if eq .Env.TMUX \"\" }}{{ if .SSHSession }} {{ end }}{{ if or .SSHSession .Root }}{{ .UserName }}{{end}}{{ if .SSHSession }}@{{ .HostName }}{{ end }}{{ end }} ",
					"trailing_diamond": "",
					"type": "session"
				},
				{
					"background": "p:blue",
					"foreground": "p:base3",
					"properties": {
						"style": "full"
					},
					"style": "powerline",
					"template": "{{ if not .Segments.Session.SSHSession }} {{ path .Path .Location }} {{ end }}",
					"type": "path"
				},
				{
					"background": "p:blue",
					"foreground": "p:base3",
					"powerline_symbol": "",
					"properties": {
						"style": "full"
					},
					"style": "powerline",
					"template": "{{ if .Segments.Session.SSHSession }} {{ path .Path .Location }} {{ end }}",
					"type": "path"
				},
				{
					"background": "p:green",
					"background_templates": [
						"{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
						"{{ if gt .Ahead 0 }}p:violet{{ end }}",
						"{{ if gt .Behind 0 }}p:magenta{{ end }}"
					],
					"foreground": "p:base3",
					"foreground_templates": [
						"{{ if or (.Working.Changed) (.Staging.Changed) }}p:base3{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:base3{{ end }}",
						"{{ if gt .Ahead 0 }}p:base3{{ end }}"
					],
					"powerline_symbol": "",
					"properties": {
						"branch_max_length": 25,
						"fetch_status": true,
						"fetch_upstream_icon": true
					},
					"style": "accordion",
					"template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }} ",
					"type": "git"
				}
			],
			"type": "prompt"
		},
		{
			"segments": [
				{
					"background": "transparent",
					"foreground": "p:green",
					"properties": {
						"display_mode": "files",
						"fetch_package_manager": false,
						"home_enabled": false
					},
					"style": "plain",
					"template": " ",
					"type": "node"
				},
				{
					"background": "transparent",
					"foreground": "p:blue",
					"properties": {
						"fetch_version": false
					},
					"style": "plain",
					"template": " ",
					"type": "go"
				},
				{
					"background": "transparent",
					"foreground": "p:yellow",
					"properties": {
						"display_mode": "files",
						"fetch_version": false,
						"fetch_virtual_env": false
					},
					"style": "plain",
					"template": " ",
					"type": "python"
				},
				{
					"type": "docker",
					"style": "plain",
					"foreground": "p:blue",
					"template": "  {{ .Context }} "
				},
				{
					"type": "text",
					"style": "plain",
					"foreground": "p:yellow",
					"template": "{{ if ne .Env.BG_JOBS \"0\" }}  {{ end }}"
				},
				{
					"background": "p:red",
					"foreground": "p:base3",
					"invert_powerline": true,
					"powerline_symbol": "",
					"style": "powerline",
					"template": " {{ .Code }} ",
					"type": "status"
				},
				{
					"background": "p:bgHighlight",
					"foreground": "p:comment",
					"invert_powerline": true,
					"powerline_symbol": "",
					"properties": {
						"always_enabled": true
					},
					"style": "powerline",
					"type": "executiontime"
				},
				{
					"background": "p:bg",
					"foreground": "p:comment",
					"invert_powerline": true,
					"style": "diamond",
					"template": "{{ .CurrentDate | date \"15:04:05\" }}",
					"type": "time"
				}
			],
			"type": "rprompt"
		}
	],
	"console_title_template": "{{ .Shell }} in {{ .Folder }}",
	"enable_cursor_positioning": true,
	"final_space": true,
	"palette": {
		"base00": "#657B83",
		"base01": "#586E75",
		"base02": "#073642",
		"base03": "#002B36",
		"base0": "#839496",
		"base1": "#93A1A1",
		"base2": "#EEE8D5",
		"base3": "#FDF6E3",
		"blue": "#268BD2",
		"cyan": "#2AA198",
		"green": "#859900",
		"magenta": "#D33682",
		"orange": "#CB4B16",
		"red": "#DC322F",
		"violet": "#6C71C4",
		"yellow": "#B58900"
	},
	"palettes": {
		"list": {
			"dark": {
				"bg": "p:base03",
				"bgHighlight": "p:base02",
				"comment": "p:base01"
			},
			"light": {
				"bg": "p:base3",
				"bgHighlight": "p:base2",
				"comment": "p:base1"
			}
		},
		"template": "{{ if eq .Env.APPEARANCE \"dark\" }}dark{{ else }}light{{ end }}"
	},
	"secondary_prompt": {
		"background": "transparent",
		"foreground": "p:base03",
		"template": "<p:base3,p:blue> > </><p:blue,transparent></> "
	},
	"tooltips": [
		{
			"background": "p:orange",
			"foreground": "p:base3",
			"leading_diamond": "",
			"properties": {
				"display_default": true
			},
			"style": "diamond",
			"template": "  {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
			"tips": ["aws"],
			"trailing_diamond": "",
			"type": "aws"
		},
		{
			"background": "p:blue",
			"foreground": "p:base3",
			"leading_diamond": "",
			"properties": {
				"display_default": true
			},
			"style": "diamond",
			"template": "  {{ .Name }} ",
			"tips": ["az"],
			"trailing_diamond": "",
			"type": "az"
		}
	],
	"upgrade": {
		"auto": false,
		"interval": "168h",
		"notice": false,
		"source": "cdn"
	},
	"version": 3
}
